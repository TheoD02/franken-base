{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "archtechx/enums": "^1.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.18",
        "knplabs/knp-paginator-bundle": "^6.3",
        "mark-gerarts/automapper-plus-bundle": "^1.4",
        "nelmio/api-doc-bundle": "^4.20",
        "nyholm/psr7": "^1.8",
        "pentatrion/vite-bundle": "^6.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sentry/sentry-symfony": "^4.14",
        "symfony/console": "~6.4.0",
        "symfony/dotenv": "~6.4.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "~6.4.0",
        "symfony/messenger": "~6.4.0",
        "symfony/property-access": "~6.4.0",
        "symfony/property-info": "~6.4.0",
        "symfony/runtime": "~6.4.0",
        "symfony/scheduler": "~6.4.0",
        "symfony/serializer": "~6.4.0",
        "symfony/translation": "~6.4.0",
        "symfony/twig-bundle": "~6.4.0",
        "symfony/validator": "~6.4.0",
        "symfony/yaml": "~6.4.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.4",
        "ergebnis/composer-normalize": "^2.42",
        "ergebnis/phpunit-slow-test-detector": "^2.10",
        "osteel/openapi-httpfoundation-testing": "^0.11.0",
        "phpunit/phpunit": "^10.0",
        "symfony/browser-kit": "~6.4.0",
        "symfony/css-selector": "~6.4.0",
        "symfony/debug-bundle": "~6.4.0",
        "symfony/maker-bundle": "^1.54",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "~6.4.0",
        "symfony/web-profiler-bundle": "~6.4.0",
        "zenstruck/foundry": "^1.36"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Module\\Api\\": "modules/api/src/",
            "Module\\ExceptionHandlerBundle\\": "modules/exception/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
